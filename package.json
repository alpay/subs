{
  "name": "subs",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/user/repo-name.git"
  },
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "prebuild": "pnpm expo prebuild",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "web": "expo start --web",
    "xcode": "xed -b ios",
    "doctor": "npx expo-doctor@latest",
    "preinstall": "npx only-allow pnpm",
    "prebuild:development": "cross-env EXPO_PUBLIC_APP_ENV=development STRICT_ENV_VALIDATION=1 pnpm run prebuild",
    "start:production": "cross-env EXPO_PUBLIC_APP_ENV=production pnpm run start",
    "prebuild:production": "cross-env EXPO_PUBLIC_APP_ENV=production STRICT_ENV_VALIDATION=1 pnpm run prebuild",
    "ios:production": "cross-env EXPO_PUBLIC_APP_ENV=production pnpm run ios",
    "build:development:ios": "eas build --profile development --platform ios",
    "build:production:ios": "eas build --profile production --platform ios",
    "submit:production:ios": "eas submit --profile production --platform ios",
    "build:local:ios": "eas build --profile production --platform ios --local --output ./build/subs.ipa",
    "submit:local:ios": "eas submit --profile production --platform ios --path ./build/subs.ipa",
    "app-release": "cross-env SKIP_BRANCH_PROTECTION=true np --no-publish --no-cleanup --no-release-draft",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noemit",
    "lint:translations": "eslint ./src/translations/ --fix --ext .json",
    "test": "jest",
    "check-all": "pnpm run lint && pnpm run type-check && pnpm run lint:translations && pnpm run test",
    "test:ci": "pnpm run test --coverage",
    "test:watch": "pnpm run test --watch",
    "install-maestro": "curl -Ls 'https://get.maestro.mobile.dev' | bash",
    "e2e-test": "maestro test .maestro/ -e APP_ID=com.obytes.development",
    "android": "expo run:android"
  },
  "dependencies": {
    "@expo/metro-runtime": "^55.0.4",
    "@expo/ui": "55.0.0-preview.5",
    "@expo/vector-icons": "^15.0.3",
    "@mgcrea/react-native-swiftui": "^0.15.2",
    "@oleg_svetlichnyi/expo-icloud-storage": "^1.1.0",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^5.90.19",
    "add": "^2.0.6",
    "app-icon-badge": "^0.1.2",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "expo": "55.0.0-preview.9",
    "expo-blur": "^55.0.4",
    "expo-constants": "~55.0.3",
    "expo-dev-client": "~55.0.4",
    "expo-document-picker": "^55.0.4",
    "expo-file-system": "^55.0.4",
    "expo-font": "~55.0.3",
    "expo-glass-effect": "^0.1.8",
    "expo-haptics": "^55.0.4",
    "expo-image": "~55.0.3",
    "expo-image-picker": "^55.0.4",
    "expo-linear-gradient": "^55.0.4",
    "expo-linking": "~55.0.3",
    "expo-localization": "~55.0.4",
    "expo-media-library": "^55.0.4",
    "expo-notifications": "^55.0.5",
    "expo-router": "55.0.0-preview.6",
    "expo-sharing": "^14.0.8",
    "expo-splash-screen": "~55.0.4",
    "expo-status-bar": "~55.0.2",
    "expo-store-review": "^9.0.9",
    "expo-symbols": "^1.0.8",
    "expo-system-ui": "~55.0.4",
    "expo-updates": "~55.0.6",
    "heroui-native": "1.0.0-beta.13",
    "i18next": "^25.8.0",
    "lodash.memoize": "^4.1.2",
    "lottie-react-native": "^7.3.6",
    "lucide-react-native": "^0.563.0",
    "moti": "^0.30.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "^6.1.0",
    "react-i18next": "^16.5.3",
    "react-native": "0.83.1",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-image-colors": "^2.5.1",
    "react-native-keyboard-controller": "1.20.4",
    "react-native-mmkv": "~4.1.1",
    "react-native-nitro-modules": "^0.33.2",
    "react-native-reanimated": "~4.2.1",
    "react-native-restart": "0.0.27",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.22.0",
    "react-native-svg": "~15.15.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "^0.7.2",
    "react-query-kit": "^3.3.2",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "uniwind": "^1.2.4",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@babel/core": "^7.28.6",
    "@dev-plugins/react-query": "^0.4.0",
    "@eslint-react/eslint-plugin": "^2.7.2",
    "@expo/config": "~55.0.4",
    "@tailwindcss/cli": "^4.0.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/i18n-js": "^4.0.1",
    "@types/invariant": "^2.2.37",
    "@types/jest": "^29.5.14",
    "@types/lodash.memoize": "^4.1.9",
    "@types/react": "~19.2.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~55.0.3",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-better-tailwindcss": "^4.0.1",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~55.0.6",
    "jest-junit": "^16.0.0",
    "np": "^10.3.0",
    "tailwindcss": "4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "react-native-restart"
        ]
      }
    },
    "install": {
      "exclude": [
        "eslint-config-expo",
        "react-native-worklets"
      ]
    }
  },
  "osMetadata": {
    "initVersion": "9.0.0"
  }
}
