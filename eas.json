{
  "cli": { "appVersionSource": "remote" },
  "build": {
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "distribution": "store",
      "environment": "production",
      "ios": { "image": "latest" },
      "android": { "buildType": "app-bundle", "image": "latest" }
    },
    "simulator": {
      "environment": "production",
      "ios": { "simulator": true, "image": "latest" },
      "android": { "image": "latest" }
    }
  },
  "submit": { "production": {} }
}
